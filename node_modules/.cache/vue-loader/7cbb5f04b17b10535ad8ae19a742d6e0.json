ourcesContent":["<template>\n  <div class=\"home\">\n    <!-- <Slider /> -->\n    <hr class=\"my-3\" />\n    <router-link class=\"btn btn-primary\" to=\"/createsemesters\"\n      >Add semester</router-link\n    >\n    <table class=\"table tble-striped\">\n      <thead>\n        <tr>\n          <th scope=\"col\">id</th>\n          <th scope=\"col\">Nama semester</th>\n          <th scope=\"col\">Sks</th>\n          <th scope=\"col\">Aksi</th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr v-for=\"(semester, index) in semesters\" :key=\"index\">\n          <td>{{ semester.id }}</td>\n          <td>{{ semester.nama_semester }}</td>\n          <td>{{ semester.sks }}</td>\n\n          <td>\n            <router-link\n              class=\"btn btn-success\"\n              :to=\"{ name: 'Editsemester', params: { id: semester.id } }\"\n              >Edit</router-link\n            >\n            <button\n              @click.prevent=\"semestersDelete(semester.id)\"\n              class=\"btn btn-danger\"\n            >\n              delete\n            </button>\n          </td>\n        </tr>\n      </tbody>\n    </table>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n// @ is an alias to /src\nimport { onMounted, ref } from \"vue\";\nexport default {\n  setup() {\n    let semesters = ref([]);\n    onMounted(() => {\n      axios\n        .get(\"http://127.0.0.1:8000/api/semesters\")\n        .then((response) => {\n          console.log(response.data.data);\n          semesters.value = response.data.data;\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    });\n    function semestersDelete(id) {\n      axios\n        .delete(`http://127.0.0.1:8000/api/semesters/delete/${id}`)\n        .then(() => {\n          let z = this.semesters.map((semesters) => semesters.id).indexOf(id);\n          this.semesters.splice(z, 1);\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    }\n    return {\n      semesters,\n      semestersDelete,\n    };\n  },\n};\n</script>"]}]}