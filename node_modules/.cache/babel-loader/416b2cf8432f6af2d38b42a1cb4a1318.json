AnB,EAAsB,aAA9C;AACD,OAPH,EAQG,KARH,CAQS,UAAC,KAAD,EAAW;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,KAAK,CAAC,QAAN,CAAe,IAA3B;AACD,OAVH;AAWD,KAZQ,CAAT;;AAaA,aAAS,MAAT,GAAkB;AAChB,UAAI,EAAC,GAAI,OAAO,CAAC,EAAjB;AACA,UAAI,MAAK,GAAI,OAAO,CAAC,MAArB;AACA,UAAI,aAAY,GAAI,OAAO,CAAC,aAA5B;AAEA,MAAA,KAAI,CACD,GADH,oDACmD,EADnD,GACyD;AACrD,QAAA,EAAE,EAAE,EADiD;AAErD,QAAA,MAAM,EAAE,MAF6C;AAGrD,QAAA,aAAa,EAAE;AAHsC,OADzD,EAMG,IANH,CAMQ,YAAM;AACV,QAAA,MAAM,CAAC,IAAP,CAAY;AACV,UAAA,IAAI,EAAE;AADI,SAAZ;AAGD,OAVH,EAWG,KAXH,CAWS,UAAC,KAAD,EAAW;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OAbH;AAcF;;AACA,WAAO;AACL,MAAA,OAAO,EAAP,OADK;AAEL,MAAA,UAAU,EAAV,UAFK;AAGL,MAAA,MAAM,EAAN,MAHK;AAIL,MAAA,MAAM,EAAN,MAJK;AAKL,MAAA,KAAK,EAAL;AALK,KAAP;AAOD;AAnDY,CAAf","sourcesContent":["<template>\n  <div class=\"card shadow mt-3\">\n    <div class=\"card-body\">\n      <h5 class=\"card-title\" style=\"color: blue\">Edit jadwal</h5>\n      <form class=\"row g-3\" @submit.prevent=\"update\">\n        <!-- <div class=\"col-md-6\">\n          <label for=\"inputEmail4\" class=\"form-label\">id</label>\n          <input\n            type=\"number\"\n            class=\"form-control\"\n            id=\"inputEmail4\"\n            v-model=\"jadwals.id\"\n          />\n          <div class=\"alert alert-danger\" v-if=\"validation.id\">\n            {{ validation.id[0] }}\n          </div>\n        </div> -->\n        <div class=\"col-md-12\">\n          <label for=\"inputPassword4\" class=\"form-label\">Jadwal</label>\n          <input\n            type=\"text\"\n            class=\"form-control\"\n            id=\"inputPassword4\"\n            v-model=\"jadwals.jadwal\"\n          />\n          <div class=\"alert alert-danger\" v-if=\"validation.jadwal\">\n            {{ validation.jadwal[0] }}\n          </div>\n        </div>\n        <div class=\"col-12\">\n          <label for=\"inputAddress\" class=\"form-label\">ID Matakuliah</label>\n          <input\n            type=\"text\"\n            class=\"form-control\"\n            id=\"inputAddress\"\n            v-model=\"jadwals.matakuliah_id\"\n          />\n          <div class=\"alert alert-danger\" v-if=\"validation.matakuliah_id\">\n            {{ validation.matakuliah_id[0] }}\n          </div>\n        </div>\n        <div class=\"col-12\">\n          <button type=\"submit\" class=\"btn btn-primary\">Submit</button>\n        </div>\n      </form>\n    </div>\n  </div>\n</template>\n<script>\nimport { onMounted, reactive, ref } from \"vue\";\nimport { useRouter } from \"vue-router\";\nimport axios from \"axios\";\nimport { useRoute } from \"vue-router\";\nexport default {\n  setup() {\n    const jadwals = reactive({\n      id: \"\",\n      jadwal: \"\",\n      matakuliah_id: \"\",\n    });\n    const validation = ref([]);\n    const router = useRouter();\n    const route = useRoute();\n    console.log(\"ini id \", route.params.id);\n    onMounted(() => {\n      axios\n        .get(`http://127.0.0.1:8000/api/jadwals-only/${route.params.id}`)\n        .then((response) => {\n          console.log(response.data.data);\n          jadwals.id = response.data.data[0].id;\n          jadwals.jadwal = response.data.data[0].jadwal;\n          jadwals.matakuliah_id = response.data.data[0].matakuliah_id;\n        })\n        .catch((error) => {\n          console.log(error.response.data);\n        });\n    });\n    function update() {\n      let id = jadwals.id;\n      let jadwal = jadwals.jadwal;\n      let matakuliah_id = jadwals.matakuliah_id;\n\n      axios\n        .put(`http://127.0.0.1:8000/api/jadwals/update/${id}`, {\n          id: id,\n          jadwal: jadwal,\n          matakuliah_id: matakuliah_id,\n        })\n        .then(() => {\n          router.push({\n            name: \"Jadwal\",\n          });\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    }\n    return {\n      jadwals,\n      validation,\n      router,\n      update,\n      route,\n    };\n  },\n};\n</script>\n"],"sourceRoot":""}]}