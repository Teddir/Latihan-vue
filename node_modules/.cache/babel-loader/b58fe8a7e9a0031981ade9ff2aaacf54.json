AmB,CAAnB,EAAsB,WAA7C;AACD,OAPH,EAQG,KARH,CAQS,UAAC,KAAD,EAAW;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,KAAK,CAAC,QAAN,CAAe,IAA3B;AACD,OAVH;AAWD,KAZQ,CAAT;;AAaA,aAAS,MAAT,GAAkB;AAChB,UAAI,EAAC,GAAI,QAAQ,CAAC,EAAlB;AACA,UAAI,YAAW,GAAI,QAAQ,CAAC,YAA5B;AACA,UAAI,WAAU,GAAI,QAAQ,CAAC,WAA3B;AAEA,MAAA,KAAI,CACD,GADH,qDACoD,EADpD,GAC0D;AACtD,QAAA,EAAE,EAAE,EADkD;AAEtD,QAAA,YAAY,EAAE,YAFwC;AAGtD,QAAA,WAAW,EAAE;AAHyC,OAD1D,EAMG,IANH,CAMQ,YAAM;AACV,QAAA,MAAM,CAAC,IAAP,CAAY;AACV,UAAA,IAAI,EAAE;AADI,SAAZ;AAGD,OAVH,EAWG,KAXH,CAWS,UAAC,KAAD,EAAW;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OAbH;AAcF;;AACA,WAAO;AACL,MAAA,QAAQ,EAAR,QADK;AAEL,MAAA,UAAU,EAAV,UAFK;AAGL,MAAA,MAAM,EAAN,MAHK;AAIL,MAAA,MAAM,EAAN,MAJK;AAKL,MAAA,KAAK,EAAL;AALK,KAAP;AAOD;AAnDY,CAAf","sourcesContent":["<template>\n  <div class=\"card shadow mt-3\">\n    <div class=\"card-body\">\n      <h5 class=\"card-title\" style=\"color: blue\">Edit kontrak</h5>\n      <form class=\"row g-3\" @submit.prevent=\"update\">\n        <!-- <div class=\"col-md-6\">\n          <label for=\"inputEmail4\" class=\"form-label\">id</label>\n          <input\n            type=\"number\"\n            class=\"form-control\"\n            id=\"inputEmail4\"\n            v-model=\"kontraks.id\"\n          />\n          <div class=\"alert alert-danger\" v-if=\"validation.id\">\n            {{ validation.id[0] }}\n          </div>\n        </div> -->\n        <div class=\"col-md-12\">\n          <label for=\"inputPassword4\" class=\"form-label\">ID Mahasiswa</label>\n          <input\n            type=\"text\"\n            class=\"form-control\"\n            id=\"inputPassword4\"\n            v-model=\"kontraks.mahasiswa_id\"\n          />\n          <div class=\"alert alert-danger\" v-if=\"validation.mahasiswa_id\">\n            {{ validation.mahasiswa_id[0] }}\n          </div>\n        </div>\n        <div class=\"col-12\">\n          <label for=\"inputAddress\" class=\"form-label\">ID Semester</label>\n          <input\n            type=\"text\"\n            class=\"form-control\"\n            id=\"inputAddress\"\n            v-model=\"kontraks.semester_id\"\n          />\n          <div class=\"alert alert-danger\" v-if=\"validation.semester_id\">\n            {{ validation.semester_id[0] }}\n          </div>\n        </div>\n        <div class=\"col-12\">\n          <button type=\"submit\" class=\"btn btn-primary\">Submit</button>\n        </div>\n      </form>\n    </div>\n  </div>\n</template>\n<script>\nimport { onMounted, reactive, ref } from \"vue\";\nimport { useRouter } from \"vue-router\";\nimport axios from \"axios\";\nimport { useRoute } from \"vue-router\";\nexport default {\n  setup() {\n    const kontraks = reactive({\n      id: \"\",\n      mahasiswa_id: \"\",\n      semester_id: \"\",\n    });\n    const validation = ref([]);\n    const router = useRouter();\n    const route = useRoute();\n    console.log(\"ini id \", route.params.id);\n    onMounted(() => {\n      axios\n        .get(`http://127.0.0.1:8000/api/kontraks-only/${route.params.id}`)\n        .then((response) => {\n          console.log(response.data.data);\n          kontraks.id = response.data.data[0].id;\n          kontraks.mahasiswa_id = response.data.data[0].mahasiswa_id;\n          kontraks.semester_id = response.data.data[0].semester_id;\n        })\n        .catch((error) => {\n          console.log(error.response.data);\n        });\n    });\n    function update() {\n      let id = kontraks.id;\n      let mahasiswa_id = kontraks.mahasiswa_id;\n      let semester_id = kontraks.semester_id;\n\n      axios\n        .put(`http://127.0.0.1:8000/api/kontraks/update/${id}`, {\n          id: id,\n          mahasiswa_id: mahasiswa_id,\n          semester_id: semester_id,\n        })\n        .then(() => {\n          router.push({\n            name: \"Kontrak\",\n          });\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    }\n    return {\n      kontraks,\n      validation,\n      router,\n      update,\n      route,\n    };\n  },\n};\n</script>\n"],"sourceRoot":""}]}