AA,CAWM,KAXN,EAAA,WAAA,EAWM,CAVJ,WAUI,E,gBATJ,YAAA,CAKE,OALF,EAKE;AAJA,IAAA,IAAI,EAAC,MAIL;AAHA,IAAA,KAAK,EAAC,cAGN;AAFA,IAAA,EAAE,EAAC,gBAEH;;aADS,MAAA,CAAA,QAAA,CAAS,UAAT,GAAmB,M;;AAC5B,GALF,E,IAAA,E;;AAAA,G,iBAIW,MAAA,CAAA,QAAA,CAAS,U,GAKhB,EAHkC,MAAA,CAAA,UAAA,CAAW,U,kBAAjD,YAAA,CAEM,KAFN,EAAA,WAAA,EAEM,gBAAA,CADD,MAAA,CAAA,UAAA,CAAW,UACV,CAFN,EAC0B;AAAA;AAD1B,G,qCAGI,CAXN,C,EAYA,W,CAzDF,E;;AAAA,GA6DI,CA/DN,CAgEI,CAjEN,C","sourcesContent":["<template>\n  <div class=\"card shadow mt-3\">\n    <div class=\"card-body\">\n      <h5 class=\"card-title\" style=\"color: blue\">Add absensi</h5>\n      <form class=\"row g-3\" @submit.prevent=\"store\">\n        <!-- <div class=\"col-md-6\">\n      <label for=\"inputEmail4\" class=\"form-label\">id</label>\n      <input type=\"number\" class=\"form-control\" id=\"inputEmail4\" \n      v-model=\"absensis.id\" />\n      <div class=\"alert alert-danger\" v-if=\"validation.id\">\n        {{ validation.id[0] }}\n      </div>\n    </div> -->\n        <div class=\"col-md-12\">\n          <label for=\"inputPassword4\" class=\"form-label\">Waktu Absen</label>\n          <input\n            type=\"text\"\n            class=\"form-control\"\n            id=\"inputPassword4\"\n            v-model=\"absensis.waktu_absen\"\n          />\n          <div class=\"alert alert-danger\" v-if=\"validation.waktu_absen\">\n            {{ validation.waktu_absen }}\n          </div>\n        </div>\n        <div class=\"col-12\">\n          <label for=\"inputAddress\" class=\"form-label\">ID Mahasiswa</label>\n          <input\n            type=\"text\"\n            class=\"form-control\"\n            id=\"inputAddress\"\n            v-model=\"absensis.mahasiswa_id\"\n          />\n          <div class=\"alert alert-danger\" v-if=\"validation.mahasiswa_id\">\n            {{ validation.mahasiswa_id }}\n          </div>\n        </div>\n        <div class=\"col-12\">\n          <label for=\"inputAddress\" class=\"form-label\">ID Matakuliah</label>\n          <input\n            type=\"text\"\n            class=\"form-control\"\n            id=\"inputAddress\"\n            v-model=\"absensis.matakuliah_id\"\n          />\n          <div class=\"alert alert-danger\" v-if=\"validation.matakuliah_id\">\n            {{ validation.matakuliah_id }}\n          </div>\n        </div>\n        <div class=\"col-md-12\">\n          <label for=\"inputPassword4\" class=\"form-label\">Keterangan</label>\n          <input\n            type=\"text\"\n            class=\"form-control\"\n            id=\"inputPassword4\"\n            v-model=\"absensis.keterangan\"\n          />\n          <div class=\"alert alert-danger\" v-if=\"validation.keterangan\">\n            {{ validation.keterangan }}\n          </div>\n        </div>\n        <div class=\"col-12\">\n          <button type=\"submit\" class=\"btn btn-primary\">Add</button>\n        </div>\n      </form>\n    </div>\n  </div>\n</template>\n<script>\nimport { reactive, ref } from \"vue\";\nimport { useRouter } from \"vue-router\";\nimport axios from \"axios\";\nexport default {\n  setup() {\n    const absensis = reactive({\n      id: \"\",\n      waktu_absen: \"\",\n      mahasiswa_id: \"\",\n      matakuliah_id: \"\",\n      keterangan: \"\",\n    });\n    const validation = ref([]);\n    const router = useRouter();\n    function store() {\n      let id = absensis.id;\n      let waktu_absen = absensis.waktu_absen;\n      let mahasiswa_id = absensis.mahasiswa_id;\n      let matakuliah_id = absensis.matakuliah_id;\n      let keterangan = absensis.keterangan;\n      axios\n        .post(\"http://127.0.0.1:8000/api/absensis/create\", {\n          id: id,\n          waktu_absen: waktu_absen,\n          mahasiswa_id: mahasiswa_id,\n          matakuliah_id: matakuliah_id,\n          keterangan: keterangan,\n        })\n        .then(() => {\n          router.push({\n            name: \"Absensi\",\n          });\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    }\n    return {\n      absensis,\n      validation,\n      router,\n      store,\n    };\n  },\n};\n</script>"],"sourceRoot":""}]}