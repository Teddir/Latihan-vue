ADK;AAEL,MAAA,aAAa,EAAb;AAFK,KAAP;AAID;AA7BY,CAAf","sourcesContent":["<template>\n  <div class=\"home\">\n    <!-- <Slider /> -->\n    <hr class=\"my-3\" />\n    <router-link class=\"btn btn-primary\" to=\"/createjadwals\"\n      >Add jadwal</router-link\n    >\n    <table class=\"table tble-striped\">\n      <thead>\n        <tr>\n          <th scope=\"col\">id</th>\n          <th scope=\"col\">jadwal</th>\n          <th scope=\"col\">matakuliah_id</th>\n          <th scope=\"col\">aksi</th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr v-for=\"(jadwal, index) in jadwals\" :key=\"index\">\n          <td>{{ jadwal.id }}</td>\n          <td>{{ jadwal.jadwal }}</td>\n          <td>{{ jadwal.matakuliah_id }}</td>\n\n          <td>\n            <router-link\n              class=\"btn btn-success\"\n              :to=\"{ name: 'Editjadwal', params: { id: jadwal.id } }\"\n              >Edit</router-link\n            >\n            <button\n              @click.prevent=\"jadwalsDelete(jadwal.id)\"\n              class=\"btn btn-danger\"\n            >\n              delete\n            </button>\n          </td>\n        </tr>\n      </tbody>\n    </table>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n// @ is an alias to /src\nimport { onMounted, ref } from \"vue\";\nexport default {\n  setup() {\n    let jadwals = ref([]);\n    onMounted(() => {\n      axios\n        .get(\"http://127.0.0.1:8000/api/jadwals\")\n        .then((response) => {\n          console.log(response.data.data);\n          jadwals.value = response.data.data;\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    });\n    function jadwalsDelete(id) {\n      axios\n        .delete(`http://127.0.0.1:8000/api/jadwals/delete/${id}`)\n        .then(() => {\n          let z = this.jadwals.map((jadwals) => jadwals.id).indexOf(id);\n          this.jadwals.splice(z, 1);\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    }\n    return {\n      jadwals,\n      jadwalsDelete,\n    };\n  },\n};\n</script>"],"sourceRoot":""}]}