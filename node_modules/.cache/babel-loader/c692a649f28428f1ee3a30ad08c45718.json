mB,CAAnB,EAAsB,cAAlD;AACA,QAAA,UAAU,CAAC,MAAX,GAAoB,QAAQ,CAAC,IAAT,CAAc,IAAd,CAAmB,CAAnB,EAAsB,MAA1C;AACA,QAAA,UAAU,CAAC,MAAX,GAAoB,QAAQ,CAAC,IAAT,CAAc,IAAd,CAAmB,CAAnB,EAAsB,MAA1C;AACA,QAAA,UAAU,CAAC,KAAX,GAAmB,QAAQ,CAAC,IAAT,CAAc,IAAd,CAAmB,CAAnB,EAAsB,KAAzC;AACD,OATH,EAUG,KAVH,CAUS,UAAC,KAAD,EAAW;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,KAAK,CAAC,QAAN,CAAe,IAA3B;AACD,OAZH;AAaD,KAdQ,CAAT;;AAeA,aAAS,MAAT,GAAkB;AAChB,UAAI,EAAC,GAAI,UAAU,CAAC,EAApB;AACA,UAAI,cAAa,GAAI,UAAU,CAAC,cAAhC;AACA,UAAI,MAAK,GAAI,UAAU,CAAC,MAAxB;AACA,UAAI,MAAK,GAAI,UAAU,CAAC,MAAxB;AACA,UAAI,KAAI,GAAI,UAAU,CAAC,KAAvB;AACA,MAAA,KAAI,CACD,GADH,uDACsD,EADtD,GAC4D;AACxD,QAAA,EAAE,EAAE,EADoD;AAExD,QAAA,cAAc,EAAE,cAFwC;AAGxD,QAAA,MAAM,EAAE,MAHgD;AAIxD,QAAA,MAAM,EAAE,MAJgD;AAKxD,QAAA,KAAK,EAAE;AALiD,OAD5D,EAQG,IARH,CAQQ,YAAM;AACV;AACA,QAAA,MAAM,CAAC,IAAP,CAAY;AACV,UAAA,IAAI,EAAE;AADI,SAAZ;AAGD,OAbH,EAcG,KAdH,CAcS,UAAC,KAAD,EAAW;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OAhBH;AAiBF;;AACA,WAAO;AACL,MAAA,UAAU,EAAV,UADK;AAEL,MAAA,UAAU,EAAV,UAFK;AAGL,MAAA,MAAM,EAAN,MAHK;AAIL,MAAA,MAAM,EAAN,MAJK;AAKL,MAAA,KAAK,EAAL;AALK,KAAP;AAOD;AA3DY,CAAf","sourcesContent":["<template>\n  <div class=\"card shadow mt-3\">\n    <div class=\"card-body\">\n      <h5 class=\"card-title\" style=\"color: blue\">Edit mahasiswa</h5>\n      <form class=\"row g-3\" @submit.prevent=\"update\">\n        <!-- <div class=\"col-md-6\">\n          <label for=\"inputEmail4\" class=\"form-label\">id</label>\n          <input\n            type=\"number\"\n            class=\"form-control\"\n            id=\"inputEmail4\"\n            v-model=\"mahasiswas.id\"\n          />\n          <div class=\"alert alert-danger\" v-if=\"validation.id\">\n            {{ validation.id[0] }}\n          </div>\n        </div> -->\n        <div class=\"col-md-12\">\n          <label for=\"inputPassword4\" class=\"form-label\">Nama mahasiswa</label>\n          <input\n            type=\"text\"\n            class=\"form-control\"\n            id=\"inputPassword4\"\n            v-model=\"mahasiswas.nama_mahasiswa\"\n          />\n          <div class=\"alert alert-danger\" v-if=\"validation.nama_mahasiswa\">\n            {{ validation.nama_mahasiswa[0] }}\n          </div>\n        </div>\n        <div class=\"col-12\">\n          <label for=\"inputAddress\" class=\"form-label\">Alamat</label>\n          <input\n            type=\"text\"\n            class=\"form-control\"\n            id=\"inputAddress\"\n            placeholder=\"Masukan alamat\"\n            v-model=\"mahasiswas.alamat\"\n          />\n          <div class=\"alert alert-danger\" v-if=\"validation.alamat\">\n            {{ validation.alamat[0] }}\n          </div>\n        </div>\n        <div class=\"col-12\">\n          <label for=\"inputAddress2\" class=\"form-label\">No tlp</label>\n          <input\n            type=\"number\"\n            class=\"form-control\"\n            id=\"inputAddress2\"\n            placeholder=\"Masukan No tlp\"\n            v-model=\"mahasiswas.no_tlp\"\n          />\n          <div class=\"alert alert-danger\" v-if=\"validation.no_tlp\">\n            {{ validation.no_tlp[0] }}\n          </div>\n        </div>\n        <div class=\"col-md-12\">\n          <label for=\"inputCity\" class=\"form-label\">Email</label>\n          <input\n            type=\"text\"\n            class=\"form-control\"\n            id=\"inputCity\"\n            v-model=\"mahasiswas.email\"\n          />\n          <div class=\"alert alert-danger\" v-if=\"validation.email\">\n            {{ validation.email[0] }}\n          </div>\n        </div>\n\n        <div class=\"col-12\">\n          <button type=\"submit\" class=\"btn btn-primary\">Submit</button>\n        </div>\n      </form>\n    </div>\n  </div>\n</template>\n<script>\nimport { onMounted, reactive, ref } from \"vue\";\nimport { useRouter } from \"vue-router\";\nimport axios from \"axios\";\nimport { useRoute } from \"vue-router\";\nexport default {\n  setup() {\n    const mahasiswas = reactive({\n      id: \"\",\n      nama_mahasiswa: \"\",\n      alamat: \"\",\n      no_tlp: \"\",\n      email: \"\",\n    });\n    const validation = ref([]);\n    const router = useRouter();\n    const route = useRoute();\n    console.log(\"ini id\", route.params.id);\n    onMounted(() => {\n      axios\n        .get(`http://127.0.0.1:8000/api/mahasiswa-only/${route.params.id}`)\n        .then((response) => {\n          console.log(\"aishaisiaos\", response.data.data);\n          mahasiswas.id = response.data.data[0].id;\n          mahasiswas.nama_mahasiswa = response.data.data[0].nama_mahasiswa;\n          mahasiswas.alamat = response.data.data[0].alamat;\n          mahasiswas.no_tlp = response.data.data[0].no_tlp;\n          mahasiswas.email = response.data.data[0].email;\n        })\n        .catch((error) => {\n          console.log(error.response.data);\n        });\n    });\n    function update() {\n      let id = mahasiswas.id;\n      let nama_mahasiswa = mahasiswas.nama_mahasiswa;\n      let alamat = mahasiswas.alamat;\n      let no_tlp = mahasiswas.no_tlp;\n      let email = mahasiswas.email;\n      axios\n        .put(`http://127.0.0.1:8000/api/mahasiswas/update/${id}`, {\n          id: id,\n          nama_mahasiswa: nama_mahasiswa,\n          alamat: alamat,\n          no_tlp: no_tlp,\n          email: email,\n        })\n        .then(() => {\n          // console.log(\"asasasssasa\");\n          router.push({\n            name: \"Home\",\n          });\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    }\n    return {\n      mahasiswas,\n      validation,\n      router,\n      update,\n      route,\n    };\n  },\n};\n</script>\n\n\n"],"sourceRoot":""}]}