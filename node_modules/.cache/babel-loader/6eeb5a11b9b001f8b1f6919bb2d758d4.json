AAA,QAAQ,CAAC,YAAT,GAAwB,QAAQ,CAAC,IAAT,CAAc,IAAd,CAAmB,CAAnB,EAAsB,YAA9C;AACA,QAAA,QAAQ,CAAC,aAAT,GAAyB,QAAQ,CAAC,IAAT,CAAc,IAAd,CAAmB,CAAnB,EAAsB,aAA/C;AACA,QAAA,QAAQ,CAAC,UAAT,GAAsB,QAAQ,CAAC,IAAT,CAAc,IAAd,CAAmB,CAAnB,EAAsB,UAA5C;AACD,OATH,EAUG,KAVH,CAUS,UAAC,KAAD,EAAW;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,KAAK,CAAC,QAAN,CAAe,IAA3B;AACD,OAZH;AAaD,KAdQ,CAAT;;AAeA,aAAS,MAAT,GAAkB;AAChB,UAAI,EAAC,GAAI,QAAQ,CAAC,EAAlB;AACA,UAAI,WAAU,GAAI,QAAQ,CAAC,WAA3B;AACA,UAAI,YAAW,GAAI,QAAQ,CAAC,YAA5B;AACA,UAAI,aAAY,GAAI,QAAQ,CAAC,aAA7B;AACA,UAAI,UAAS,GAAI,QAAQ,CAAC,UAA1B;AAEA,MAAA,KAAI,CACD,GADH,qDACoD,EADpD,GAC0D;AACtD,QAAA,EAAE,EAAE,EADkD;AAEtD,QAAA,WAAW,EAAE,WAFyC;AAGtD,QAAA,YAAY,EAAE,YAHwC;AAItD,QAAA,aAAa,EAAE,aAJuC;AAKtD,QAAA,UAAU,EAAE;AAL0C,OAD1D,EAQG,IARH,CAQQ,YAAM;AACV,QAAA,MAAM,CAAC,IAAP,CAAY;AACV,UAAA,IAAI,EAAE;AADI,SAAZ;AAGD,OAZH,EAaG,KAbH,CAaS,UAAC,KAAD,EAAW;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OAfH;AAgBF;;AACA,WAAO;AACL,MAAA,QAAQ,EAAR,QADK;AAEL,MAAA,UAAU,EAAV,UAFK;AAGL,MAAA,MAAM,EAAN,MAHK;AAIL,MAAA,MAAM,EAAN,MAJK;AAKL,MAAA,KAAK,EAAL;AALK,KAAP;AAOD;AA3DY,CAAf","sourcesContent":["<template>\n  <div class=\"card shadow mt-3\">\n    <div class=\"card-body\">\n      <h5 class=\"card-title\" style=\"color: blue\">Edit absensi</h5>\n      <form class=\"row g-3\" @submit.prevent=\"update\">\n        <!-- <div class=\"col-md-6\">\n          <label for=\"inputEmail4\" class=\"form-label\">id</label>\n          <input\n            type=\"number\"\n            class=\"form-control\"\n            id=\"inputEmail4\"\n            v-model=\"absensis.id\"\n          />\n          <div class=\"alert alert-danger\" v-if=\"validation.id\">\n            {{ validation.id[0] }}\n          </div>\n        </div> -->\n        <!-- <div class=\"col-md-6\">\n          <label for=\"inputEmail4\" class=\"form-label\">id</label>\n          <input\n            type=\"number\"\n            class=\"form-control\"\n            id=\"inputEmail4\"\n            v-model=\"absensis.id\"\n          />\n          <div class=\"alert alert-danger\" v-if=\"validation.id\">\n            {{ validation.id[0] }}\n          </div>\n        </div> -->\n        <div class=\"col-md-12\">\n          <label for=\"inputPassword4\" class=\"form-label\">Waktu Absen</label>\n          <input\n            type=\"text\"\n            class=\"form-control\"\n            id=\"inputPassword4\"\n            v-model=\"absensis.waktu_absen\"\n          />\n          <div class=\"alert alert-danger\" v-if=\"validation.waktu_absen\">\n            {{ validation.waktu_absen[0] }}\n          </div>\n        </div>\n        <div class=\"col-12\">\n          <label for=\"inputAddress\" class=\"form-label\">ID Mahasiswa</label>\n          <input\n            type=\"text\"\n            class=\"form-control\"\n            id=\"inputAddress\"\n            v-model=\"absensis.mahasiswa_id\"\n          />\n          <div class=\"alert alert-danger\" v-if=\"validation.mahasiswa_id\">\n            {{ validation.mahasiswa_id[0] }}\n          </div>\n        </div>\n        <div class=\"col-12\">\n          <label for=\"inputAddress\" class=\"form-label\">ID matakuliah</label>\n          <input\n            type=\"text\"\n            class=\"form-control\"\n            id=\"inputAddress\"\n            v-model=\"absensis.matakuliah_id\"\n          />\n          <div class=\"alert alert-danger\" v-if=\"validation.matakuliah_id\">\n            {{ validation.matakuliah_id[0] }}\n          </div>\n        </div>\n        <div class=\"col-md-12\">\n          <label for=\"inputPassword4\" class=\"form-label\">Keterangan</label>\n          <input\n            type=\"text\"\n            class=\"form-control\"\n            id=\"inputPassword4\"\n            v-model=\"absensis.keterangan\"\n          />\n          <div class=\"alert alert-danger\" v-if=\"validation.keterangan\">\n            {{ validation.keterangan }}\n          </div>\n        </div>\n        <div class=\"col-12\">\n          <button type=\"submit\" class=\"btn btn-primary\">Submit</button>\n        </div>\n      </form>\n    </div>\n  </div>\n</template>\n<script>\nimport { onMounted, reactive, ref } from \"vue\";\nimport { useRouter } from \"vue-router\";\nimport axios from \"axios\";\nimport { useRoute } from \"vue-router\";\nexport default {\n  setup() {\n    const absensis = reactive({\n      id: \"\",\n      waktu_absen: \"\",\n      mahasiswa_id: \"\",\n      matakuliah_id: \"\",\n      keterangan: \"\",\n    });\n    const validation = ref([]);\n    const router = useRouter();\n    const route = useRoute();\n    console.log(\"ini id \", route.params.id);\n    onMounted(() => {\n      axios\n        .get(`http://127.0.0.1:8000/api/absensis-only/${route.params.id}`)\n        .then((response) => {\n          console.log(response.data.data);\n          absensis.id = response.data.data[0].id;\n          absensis.waktu_absen = response.data.data[0].waktu_absen;\n          absensis.mahasiswa_id = response.data.data[0].mahasiswa_id;\n          absensis.matakuliah_id = response.data.data[0].matakuliah_id;\n          absensis.keterangan = response.data.data[0].keterangan;\n        })\n        .catch((error) => {\n          console.log(error.response.data);\n        });\n    });\n    function update() {\n      let id = absensis.id;\n      let waktu_absen = absensis.waktu_absen;\n      let mahasiswa_id = absensis.mahasiswa_id;\n      let matakuliah_id = absensis.matakuliah_id;\n      let keterangan = absensis.keterangan;\n\n      axios\n        .put(`http://127.0.0.1:8000/api/absensis/update/${id}`, {\n          id: id,\n          waktu_absen: waktu_absen,\n          mahasiswa_id: mahasiswa_id,\n          matakuliah_id: matakuliah_id,\n          keterangan: keterangan,\n        })\n        .then(() => {\n          router.push({\n            name: \"Absensi\",\n          });\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    }\n    return {\n      absensis,\n      validation,\n      router,\n      update,\n      route,\n    };\n  },\n};\n</script>\n"],"sourceRoot":""}]}